<hr class="my-5" />

              <!-- Hoverable Table rows -->
              <div class="card mx-4">
                <h5 class="card-header">Coupens</h5>
                <div class="table-responsive text-nowrap">
                  <table class="table table-hover">
                    <thead>
                      <tr>
                        <th>Coupen</th>
                        <th>Coupen Type</th>
                        <th>minimum Price</th>
                        <th>Discount</th>
                        <th>Max Redeem</th>
                        <th>Expiry</th>
                        <th>Status</th>
                        <th>Actions</th>
                      </tr>
                    </thead>
                    <tbody class="table-border-bottom-0">
                      {{#each coupens}}
                      <tr>
                        <td>{{this.couponCode}}</td>
                        <td>
                          {{this.type}}
                        </td>
                        <td>{{this.minimumPrice}}</td>
                        <td>{{this.discount}}</td>
                        <td>{{this.maxRedeem}}</td>
                        <td>{{this.expiry}}</td>
                        <td>
                          {{#if this.status}}
                          <p>Listed</p>
                          {{else}}
                          <p>Unisted</p>
                          </td>
                          {{/if}}
                        <td>
                          <div class="dropdown">
                            <button type="button" class="btn p-0 dropdown-toggle hide-arrow" data-bs-toggle="dropdown">
                              <i class="bx bx-dots-vertical-rounded"></i>
                            </button>
                            <div class="dropdown-menu">
                              <a class="dropdown-item" href="/admin/editCoupen/?id={{this._id}}"
                                ><i class="bx bx-edit me-1"></i> Edit</a
                              >
                              {{#if this.status}}
                              <a class="dropdown-item" href="/admin/unlistCoupen/?id={{this._id}}&_method=PATCH"
                                ><i class="bx bx-check me-1"></i>Unlist </a
                              >
                              {{else}}
                              <a class="dropdown-item" href="/admin/unlistCoupen/?id={{this._id}}&_method=PATCH">
                                ><i class="bx bx-check me-1"></i>List </a
                              >
                              {{/if}}
                              <a class="dropdown-item" href="/admin/deleteCoupen/?id={{this._id}}&_method=DELETE"  onclick="return confirm('Do you want to delete {{this.name}}?')"
                                ><i class="bx bx-trash me-1"></i> Delete</a
                              >
                            </div>
                          </div>
                        </td>
                      </tr>
                      {{/each}}
                    </tbody>
                  </table>
                </div>
              </div>
              <!--/ Hoverable Table rows -->

              <hr class="my-5" />